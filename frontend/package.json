{
  "name": "projectlocker",
  "version": "1.0.0",
  "description": "Frontend for the projectlocker project",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "build": "webpack -d",
    "build:prod": "webpack -p",
    "dev": "webpack -d --watch",
    "clean": "rm -rf node_modules ../public/javascripts/bundle.js",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app",
    "lint:fix": "prettier --write app __tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest",
      "node_modules/react-multistep/.+\\.js$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-multistep)"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": ">=4",
    "jest": "^26.0.1",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "lint-staged": ">=10",
    "moxios": "^0.4.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "sinon": "^8.0.4",
    "source-map-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.3",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@types/jsonwebtoken": "^8.5.0",
    "axios": "^0.19.2",
    "css-loader": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.20.1",
    "pluto-headers": "https://gitlab.com/codmill/customer-projects/guardian/pluto-headers.git#v1.0.2-3",
    "query-string": "^6.13.1",
    "raven-js": "^3.24.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yarn": "^1.21.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}