{
  "name": "projectlocker",
  "version": "1.0.0",
  "description": "Frontend for the projectlocker project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf node_modules ../public/javascripts/bundle.js",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app",
    "lint:fix": "prettier --write app __tests__",
    "update-interfaces": "ts-interface-builder app/vidispine/item/VidispineItem.ts app/vidispine/job/VidispineJob.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(|jpg|jpeg|png)$": "<rootDir>/fileMock.js"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest",
      "node_modules/react-multistep/.+\\.js$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-multistep)"
    ],
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/chart.js": "^2.9.34",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/moxios": "^0.4.9",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sinon": "^9.0.4",
    "buffer": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "husky": ">=4",
    "jest": "^26.0.1",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "lint-staged": ">=10",
    "mini-svg-data-uri": "^1.2.3",
    "moxios": "^0.4.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "sinon": "^8.0.4",
    "source-map-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.3",
    "ts-interface-builder": "^0.2.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^7.0.5",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.0",
    "vm-browserify": "^1.1.2"
  },
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react-helmet": "^6.1.0",
    "axios": "^0.21.3",
    "chart.js": "^2.9.4",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.2.4",
    "date-fns": "^2.22.1",
    "identity-obj-proxy": "^3.0.0",
    "ini": "^1.3.6",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.20.1",
    "pluto-headers": "https://gitlab.com/codmill/customer-projects/guardian/pluto-headers.git#v1.6.3",
    "process": "^0.11.10",
    "query-string": "^6.13.1",
    "raven-js": "^3.24.1",
    "react": "^16.12.0",
    "react-beforeunload-component": "^1.2.2",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sha1-stream": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-interface-checker": "^0.1.11",
    "typescript-loader": "^1.1.3",
    "url-loader": "^4.1.0",
    "util": "^0.12.3",
    "webpack": "^5.26.0",
    "webpack-cli": "^4.5.0",
    "y18n": "^4.0.1",
    "yarn": "^1.21.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "resolutions": {
    "ini": "^1.3.6",
    "lodash": "^4.17.20",
    "y18n": "^4.0.1"
  }
}
